
<%= form_for @user, url: url_for(controller: 'registrations', action: 'update') do |f| %>

  <div class="field">
    <%= f.label :telephone %><br />
    <%= f.text_field :telephone, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </div>

  <div class="field">
    <%= f.label :write_a_few_sentences_about_yourself %><br />
    <%= f.text_field :bio %>
  </div>

  <div class="actions">
    <%= f.submit "Next" %>
  </div>
<% end %>
